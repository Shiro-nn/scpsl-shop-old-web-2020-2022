                                        <span class="m11">Стрелок #1</span>
                                        <span class="fa-solid fa-info-circle m18"></span>
                                        <span class="m22"><span class="fa-solid fa-plus"></span></span>
                                        <span class="m27"><span class="fa-solid fa-eraser"></span></span>
                                        <span class="m23">275₽</span>
                                    </div>
                                    <div class="m9" id="visual.gunner2">
                                        <div class="m10" style="background-image: url('/img/visual/images/fsp9.png');"></div>
                                        <span class="m11">Стрелок #2</span>
                                        <span class="fa-solid fa-info-circle m18"></span>
                                        <span class="m22"><span class="fa-solid fa-plus"></span></span>
                                        <span class="m27"><span class="fa-solid fa-eraser"></span></span>
                                        <span class="m23">325₽</span>
                                    </div>
                                    <div class="m9" id="visual.gunner3">
                                        <div class="m10" style="background-image: url('/img/visual/images/shotgun.png');"></div>
                                        <span class="m11">Стрелок #3</span>
                                        <span class="fa-solid fa-info-circle m18"></span>
                                        <span class="m22"><span class="fa-solid fa-plus"></span></span>
                                        <span class="m27"><span class="fa-solid fa-eraser"></span></span>
                                        <span class="m23">375₽</span>
                                    </div>
                                    <div class="m9" id="visual.scp049">
                                        <div class="m10" style="background-image: url('/img/visual/images/049_Icon.png');"></div>
                                        <span class="m11">SCP-049</span>
                                        <span class="fa-solid fa-info-circle m18"></span>
                                        <span class="m22"><span class="fa-solid fa-plus"></span></span>
                                        <span class="m27"><span class="fa-solid fa-eraser"></span></span>
                                        <span class="m23">375₽</span>
                                    </div>
                                </div>
                                <div class="m20">
                                    <label class="m21">Стоимость: <sum id="vis.sum">0</sum>₽/мес</label><br>
                                    <label class="m21">Истекает: <exp id="vis.exp">Не приобретен</exp></label>
                                </div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
